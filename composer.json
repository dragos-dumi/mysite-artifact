{
    "name": "dragos-dumi/drupal-quick-start",
    "description": "Project template for Drupal 8 sites built with BLT.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "acquia/blt": "^8.3",
        "geerlingguy/drupal-vm": "~4.3",
        "adamwulf/columnizer": "^1.6",
        "behat/mink": "^1.7",
        "behat/mink-browserkit-driver": "^1.3",
        "behat/mink-goutte-driver": "^1.2",
        "behat/mink-selenium2-driver": "^1.3",
        "behat/mink-zombie-driver": "^1.4",
        "composer/installers": "^1.2",
        "cweagans/composer-patches": "^1.6",
        "desandro/imagesloaded": "^4.1",
        "desandro/masonry": "^4.2",
        "dinbror/blazy": "^1.8",
        "drupal/memcache": "^2.0-alpha3",
        "drupal/security_review": "*",
        "drupal/shield": "^1.0.0",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/captcha": "^1.0@beta",
        "drupal/chosen": "^2.3",
        "drupal/colorbox": "^1.4",
        "drupal/config_split": "^1.0@beta",
        "drupal/core": "~8.0",
        "drupal/display_field_copy": "^1.0",
        "drupal/ds": "^3.1",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/editor_file": "^1.2",
        "drupal/facets": "^1.0@alpha",
        "drupal/facets_pretty_paths": "1.x-dev",
        "drupal/features": "^3.5",
        "drupal/field_group": "^1.0@RC",
        "drupal/file_browser": "^1.1",
        "drupal/geofield_map": "^1.10",
        "drupal/image_effects": "^1.0",
        "drupal/image_widget_crop": "^2.0",
        "drupal/imce": "^1.6",
        "drupal/inline_responsive_images": "^2.1",
        "drupal/lang_dropdown": "^2.0@beta",
        "drupal/linkit": "5.0-beta5",
        "drupal/menu_block": "^1.4",
        "drupal/menu_breadcrumb": "^1.4",
        "drupal/menu_link_attributes": "^1.0@beta",
        "drupal/metatag": "^1.2",
        "drupal/migrate_api": "^1.1",
        "drupal/migrate_plus": "^2.0",
        "drupal/migrate_tools": "^2.0",
        "drupal/paragraphs": "^1.2",
        "drupal/pathauto": "^1.0",
        "drupal/recaptcha": "^2.2",
        "drupal/redirect": "^1.0@beta",
        "drupal/reroute_email": "^1.0@beta",
        "drupal/responsive_favicons": "^1.4",
        "drupal/search_api": "^1.2",
        "drupal/search_api_autocomplete": "^1.0@alpha",
        "drupal/search_api_solr": "^1.0",
        "drupal/search_api_sorts": "^1.0@beta",
        "drupal/search_autocomplete": "^1.1",
        "drupal/slick": "^1.0",
        "drupal/slick_views": "^1.0@RC",
        "drupal/smart_trim": "^1.0",
        "drupal/social_media": "^1.0",
        "drupal/spamspan": "^1.0@alpha",
        "drupal/stage_file_proxy": "^1.0@alpha",
        "drupal/string_field_formatter": "1.x-dev",
        "drupal/views_accordion": "^1.0",
        "drupal/webform": "^5.0@beta",
        "drupal/xmlsitemap": "^1.0@alpha",
        "enyoo/dropzone": "^4.3",
        "harvesthq/chosen": "^1.6",
        "jackmoore/colorbox": "^1.6",
        "kenwheeler/slick": "^1.6",
        "drupal/bootstrap": "^3.6",
        "drupal/panelizer": "^4.0",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/entityqueue": "^1.0@alpha",
        "drupal/geolocation": "^1.11",
        "drupal/address": "^1.2",
        "drupal/cog": "^1.10",
        "drupal/views_infinite_scroll": "^1.5",
        "drupal/diff": "^1.0@RC",
        "drupal/seckit": "^1.0@alpha",
        "drupal/devel": "^1.2",
        "drupal/raven": "^2.3",
        "drupal/redis": "^1.0@RC",
        "drupal/purge_purger_http": "^1.0@beta",
        "drupal/view_mode_selector": "1.x-dev",
        "drupal/admin_toolbar": "^1.22",
        "drupal/restui": "^1.15",
        "drupal/simple_oauth": "^3.4",
        "drupal/search_kint": "^1.0",
        "d3/d3": "3.5.*",
        "components/highlightjs": "9.7.*"
    },
    "require-dev": {},
    "repositories": {
      "drupal": {
        "type": "composer",
        "url": "https://packages.drupal.org/8"
      },
      "asset-packagist": {
        "type": "composer",
        "url": "https://asset-packagist.org"
      },
      "dinbror/blazy": {
        "type": "package",
        "package": {
          "name": "dinbror/blazy",
          "version": "1.8.2",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/dinbror/blazy/archive/1.8.2.zip",
            "type": "zip"
          }
        }
      },
      "desandro/imagesloaded": {
        "type": "package",
        "package": {
          "name": "desandro/imagesloaded",
          "version": "4.1.1",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/desandro/imagesloaded/archive/v4.1.1.zip",
            "type": "zip"
          }
        }
      },
      "enyoo/dropzone": {
        "type": "package",
        "package": {
          "name": "enyoo/dropzone",
          "version": "4.3.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/enyo/dropzone/archive/v4.3.0.zip",
            "type": "zip"
          }
        }
      },
      "desandro/masonry": {
        "type": "package",
        "package": {
          "name": "desandro/masonry",
          "version": "4.2.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/desandro/masonry/archive/v4.2.0.zip",
            "type": "zip"
          }
        }
      },
      "jackmoore/colorbox": {
        "type": "package",
        "package": {
          "name": "jackmoore/colorbox",
          "version": "1.6.4",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/jackmoore/colorbox/archive/1.6.4.zip",
            "type": "zip"
          }
        }
      },
      "kenwheeler/slick": {
        "type": "package",
        "package": {
          "name": "kenwheeler/slick",
          "version": "1.6.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/kenwheeler/slick/archive/1.6.0.zip",
            "type": "zip"
          }
        }
      },
      "harvesthq/chosen": {
        "type": "package",
        "package": {
          "name": "harvesthq/chosen",
          "version": "1.6.1",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/harvesthq/chosen/releases/download/v1.6.1/chosen_v1.6.1.zip",
            "type": "zip"
          },
          "require": {
            "composer/installers": "^1.2.0"
          }
        }
      },
      "adamwulf/columnizer": {
        "type": "package",
        "package": {
          "name": "adamwulf/columnizer",
          "version": "1.6.2",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/adamwulf/Columnizer-jQuery-Plugin/archive/1.6.2.zip",
            "type": "zip"
          }
        }
      },
      "d3": {
        "type": "package",
        "package": {
          "name": "d3/d3",
          "version": "v3.5.17",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/d3/d3/archive/v3.5.17.zip",
            "type": "zip"
          }
        }
      },
      "highlightjs": {
        "type": "package",
        "package": {
          "name": "components/highlightjs",
          "version": "9.7.0",
          "type": "drupal-library",
          "dist": {
            "url": "https://github.com/components/highlightjs/archive/9.7.0.zip",
            "type": "zip"
          }
        }
      }
    },
    "extra": {
        "enable-patching": true,
        "installer-types": [
            "bower-asset",
            "npm-asset"
        ],
        "patches": {
          "drupal/core": {
            "prevent error on first config import": "https://www.drupal.org/files/issues/content_translation_config-2868294-2.patch"
          },
          "drupal/inline_responsive_images": {
            "absolute url fix": "https://www.drupal.org/files/issues/inline_responsive_images-absolute-url-bug-2867014-1.patch"
          },
          "aquia/blt": {
            "sync:files outside vm": "https://github.com/acquia/blt/commit/c1de4c471333c280817d35bd8bdd03908f322ecd.patch",
            "sync all outside vm": "https://github.com/acquia/blt/commit/f77aae7f5b7b843bc6c2afdbf380fef4d10645fc.patch"
          }
        },
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ]
        },
        "merge-plugin": {
            "include": [
              "vendor/acquia/blt/composer.required.json"
            ],
            "merge-extra": true,
            "merge-extra-deep": true,
            "merge-scripts": true,
            "replace": false,
            "ignore-duplicates": true
        }
    }
}
